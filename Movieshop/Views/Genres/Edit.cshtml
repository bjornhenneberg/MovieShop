@model MoviesShopProxy.DomainModel.Genre

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

<form method="Post" action="~/Genre/Update" class="col-md-12">
    <input type="hidden" class="form-control" name="Id" value="@Model.id" hidden>
